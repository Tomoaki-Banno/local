/*------- global rayout --------*/
* {
  font-family: "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", "メイリオ", "Meiryo", "ＭＳ Ｐゴシック", "MS P Gothic", sans-serif;
  font-size: 12px;
  line-height: 1.2;
}
body {
    background-position: center center ;
    
    margin-top: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    margin-left: 0px;
    
    padding-top: 0px;
    padding-right: 0px;
    padding-bottom: 0px;
    padding-left: 0px;
    
    font-size: 13px;
}

p {
    color: #006666;
    text-align: left;
}

p.top{
    font-size:10px;
    margin-bottom:0;
    margin-top:0px;
    text-align:right;
    padding-top:10px;
    padding-right: 10px;
    padding-bottom: 0px;
    padding-left: 0px;
}

p a{
    text-decoration:none;
    color:#006666;
}

p a:hover{
    color:#FF9933;
}

/*------- number icon --------*/
.gen_number_icon {
    font-size: 10px;
    font-weight: 700;
    color: white;
    height: 16px;
    line-height: 16px;
    padding: 0px 3px 0px 3px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
    background-color: #FA6D64;
}

/*------- like count icon --------*/
.gen_like_count {
    background: white;
    border: 1px solid #898f9c;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    color: #6a7180;
    display: inline-block;
    font-size: 11px;
    line-height: 15px;
    margin-left: 6px;
    min-width: 15px;
    padding: 0 3px;
    text-align: center;
    white-space: nowrap;
}
.gen_like_count_sub {
    height: 0px;
    left: 2px;
    position: relative;
    top: -14px;
    width: 5px;
    z-index: 2;
    display: block;
}
.gen_like_count_sub s {
    border-top-color: transparent;
    border-right-color: #898f9c;
    border-bottom-color: transparent;
    border-left-color: #898f9c;
    border-style: solid;
    border-width: 4px 5px 4px 0;
    display: block;
    position: relative;
    top: 1px;
    text-decoration: line-through;
    text-align: left;
}
.gen_like_count_sub i {
    border-top-color: transparent;
    border-right-color: #fff;
    border-bottom-color: transparent;
    border-left-color: #898f9c;
    top: -7px;
    left: 2px;
    border-style: solid;
    border-width: 4px 5px 4px 0;
    display: block;
    position: relative;
}

/*------- like button --------*/
.gen_like_button {
    background: #2ab3f6;
    background-image: -webkit-linear-gradient(top, #2ab3f6, #0086e0);
    background-image: -moz-linear-gradient(top, #2ab3f6, #0086e0);
    background-image: -ms-linear-gradient(top, #2ab3f6, #0086e0);
    background-image: -o-linear-gradient(top, #2ab3f6, #0086e0);
    background-image: linear-gradient(to bottom, #2ab3f6, #0086e0);
    -webkit-border-radius: 5;
    -moz-border-radius: 5;
    border-radius: 5px;
    color: #ffffff;
    font-size: 9px;
    padding: 1px 5px 1px 5px;
    text-decoration: none;
}
.gen_like_button:hover {
    background: #59c2ff;
    text-decoration: none;
}
.gen_like_done_button {
    -webkit-border-radius: 5;
    -moz-border-radius: 5;
    border-radius: 5px;
    color: #ffffff;
    font-size: 9px;
    background: #63beff;
    padding: 1px 5px 1px 5px;
    border: solid #d6d6d6 1px;
    text-decoration: none;
}

/*------- button --------*/
/* mini-button は common_pc.css, common_ipad.css */
input.gen_button {
    color: #050505;
    padding: 5px;
    border: 1px solid #949494;

    background: -moz-linear-gradient(top, #fff, #F1F1F1 1%, #F1F1F1 50%, #DFDFDF 99%, #ccc);
    background: -webkit-gradient(linear, left top, left bottom, from(#fff), color-stop(0.01, #F1F1F1), color-stop(0.5, #F1F1F1), color-stop(0.99, #DFDFDF), to(#ccc));

    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    
    -moz-box-shadow: 0px 1px 3px rgba(000,000,000,0.5), inset 0px 0px 2px rgba(255,255,255,1);
    -webkit-box-shadow: 0px 1px 3px rgba(000,000,000,0.5), inset 0px 0px 2px rgba(255,255e,255,1);
    box-shadow: 0px 1px 3px rgba(000,000,000,0.5), inset 0px 0px 2px rgba(255,255,255,1);
}
input.gen_button:active {
    background: #ccc;
    border: 1px solid #666666;

    -moz-box-shadow: 0px 0px 0px;
    -webkit-box-shadow: 0px 0px 0px;
    box-shadow: 0px 0px 0px;
}
input.gen_button:disabled {
    color: #6d6d6d;
    background: #efefef;
    border: 1px solid #d9d9d9;

    -moz-box-shadow: 0px 0px 0px;
    -webkit-box-shadow: 0px 0px 0px;
    box-shadow: 0px 0px 0px;
}


/*------- dialog --------*/

/* YUI Panel用。class='yui-skin-sam gen_dialog' のように指定 */
.gen_dialog .yui-panel{
    background-color: #ffffff !important;
    border-color: #cccccc !important;
    border-width: 0px 1px 1px 0px !important;
    text-align: center !important;
}

.gen_dialog .hd{
    border-color: #cccccc !important;
    border-width: 1px 1px 1px 1px !important;
    text-align: center !important;
}

.gen_dialog .bd{
    padding: 0px !important;
    background-color: #ffffff !important;
    border-color: #cccccc !important;
    border-width: 0px 1px 1px 1px !important;
    text-align: center !important;
}

.gen_dialog .underlay{
    top: 2px !important;
    left: -2px !important;
    right: 2px !important;
    bottom: -2px !important;
}

/*------- contextMenu --------*/

.contextMenu {
    position: absolute;
    width: 170px;
    z-index: 99999;
    border: solid 1px #666666;
    background: #f5f5f5;
    padding: 0px;
    margin: 0px;
}
.contextMenu LI {
    list-style: none;
    padding: 0px;
    margin: 0px;
    text-align:left;
}
/* .contextMenu A のみ common_pc.css, common_ipad.css にある */

.contextMenu LI.hover A {
    color: #000000;
    background-color: #8dc4fc;
}
.contextMenu LI.disabled A {
    color: #AAA;
    cursor: default;
}
.contextMenu LI.hover.disabled A {
    background-color: transparent;
}
.contextMenu LI.separator {
    border-top: solid 1px #CCC;
}
.contextMenu LI.norm {
    border-top: solid 1px #f5f5f5;
}

